[Unit]
Description=API GO lang
After=network.target

[Service]
Type=simple
User=root
Group=root
Restart=always
EnvironmentFile=/var/www/apigo/.env
WorkingDirectory=/var/www/apigo
ExecStart=/usr/local/go/bin/go run apigo
StandardOutput=syslog
StandardError=syslog

[Install]
WantedBy=multi-user.target
